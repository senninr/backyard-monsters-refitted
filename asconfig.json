{
	"config": "air",
	"type": "app",
	"mainClass": "GAME",
 	"compilerOptions": {
		"strict": false,
		"warnings": false,
		"language": "3",
		"output": "bin/bymr-stable.air",
		"source-path": [
			"client/scripts"
		],
		"default-background-color": "#1D232A",
		"swf-version": 11,
		"target-player": "16.0",
		"default-frame-rate": 24,
		"default-size": {
			"width": 760,
			"height": 670
		},
		"library-path": [
			"playerglobal.swc",
			"airglobal.swc"
		],
		"use-network": true,
		"static-link-runtime-shared-libraries": true
	},
	"airOptions": {
		"output": "bin/bymr-stable.exe",
		"signingOptions": {
			"storetype": "pkcs12",
			"keystore": "bin/self-signed-cert.p12",
			"tsa": "http://timestamp.digicert.com"
		}
	}
}